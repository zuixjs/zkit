{
  "zuix": {
    "build": {
      "input": "source",
      "output": "docs",
      "copy": [
        "css",
        "images",
        ".nojekyll",
        "browserconfig.xml",
        "manifest.json",
        "humans.txt",
        "favicon.ico",
        "robots.txt",
        "content/**/*.jpg",
        "content/**/*.png",
        "content/**/*.webp",
        "content/**/*.gif",
        "content/**/*.svg",
        "content/**/*.json"
      ],
      "ignore": [
        "_filters",
        "content/**/_inc/*"
      ],
      "dataFolder": "_data",
      "includesFolder": "_inc",
      "componentsFolders": [ "app", "lib" ],
      "contentFolder": "content",
      "bundle": {
        "js": true,
        "css": true,
        "zuix": true
      },
      "mirror": "script,style,img,source,audio",
      "serviceWorker": true,
      "minify": {
        "disable": false,
        "collapseWhitespace": true,
        "removeOptionalTags": true,
        "removeRedundantAttributes": true,
        "removeScriptTypeAttributes": true,
        "removeTagWhitespace": false,
        "useShortDoctype": false,
        "collapseBooleanAttributes": true,
        "removeAttributeQuotes": false,
        "removeEmptyAttributes": true,
        "minifyCSS": false,
        "minifyJS": true
      }
    },
    "app": {
      "title": "<code>zKit</code> components",
      "subtitle": "web enhancing bits",
      "googleSiteId": "UA-116384214-1",
      "siteMapUrl": "https://zuixjs.github.io/zkit",
      "baseUrl": "/zkit/",
      "resourcePath": "/zkit/app/",
      "libraryPath": {
        "@lib": "/zkit/lib/1.1/",
        "@hgui": "https://genielabs.github.io/homegenie-web-ui/app/",
        "@cdnjs": "https://cdnjs.cloudflare.com/ajax/libs/"
      },
      "zuixjs.github.io": {
        "resourcePath": "/zkit/app/",
        "libraryPath": {
          "@lib": "/zkit/lib/1.1/",
          "@hgui": "https://genielabs.github.io/homegenie-web-ui/app/",
          "@cdnjs": "https://cdnjs.cloudflare.com/ajax/libs/"
        }
      }
    }
  }
}
